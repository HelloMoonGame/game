{
  "name": "hellomoon-game",
  "author": "@hellomoon",
  "license": "GPL-3.0-only",
  "version": "1.0.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "export": "next export",
    "start": "next start",
    "type-check": "tsc --pretty --noEmit",
    "format": "prettier --write .",
    "lint": "eslint . --ext ts --ext tsx --ext js",
    "test": "jest",
    "test-all": "yarn lint && yarn type-check && yarn test",
    "generate-grpc-client": "protoc --proto_path=../../api/src/CharacterApi/Protos --js_out=import_style=commonjs,binary:grpc --grpc-web_out=import_style=typescript,mode=grpcwebtext:grpc location.proto",
    "prepare": "cd .. && husky install src/.husky"
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "yarn lint",
      "yarn format"
    ]
  },
  "dependencies": {
    "@capacitor/cli": "^2.4.7",
    "@capacitor/core": "^2.4.7",
    "@hookform/resolvers": "^2.4.0",
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@reduxjs/toolkit": "^1.5.1",
    "axios": "^0.21.1",
    "babel-polyfill": "^6.26.0",
    "google-protobuf": "^3.15.8",
    "grpc-web": "^1.2.1",
    "next": "^10.1.3",
    "oidc-client": "^1.11.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.3.4",
    "react-redux": "^7.2.4",
    "xmlhttprequest": "^1.8.0",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@testing-library/react": "^11.2.6",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.0.0",
    "@types/react": "^17.0.4",
    "@types/react-dom": "^17.0.3",
    "@types/react-redux": "^7.1.16",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.19.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "jest-watch-typeahead": "^0.6.3",
    "lint-staged": "^10.0.10",
    "prettier": "^2.0.2",
    "typescript": "^4.2.4"
  }
}
